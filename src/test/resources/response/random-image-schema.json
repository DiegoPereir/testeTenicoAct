{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string",
      "format": "uri",
      "pattern": "^https://.*\\.(jpg|jpeg|png|gif)$"
    },
    "status": {
      "type": "string",
      "enum": ["success"]
    }
  },
  "required": ["message", "status"],
  "additionalProperties": false
} 